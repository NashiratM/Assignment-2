2. Select FIRT_NAME AS WORKER_NAME from Worker;

3. Select Upper(FIRST_CASE) from Worker;

4. Select distinct DEPARTMENT from Work;

5. Select substring (FIRST_NAME, 1,3) from Worker;

6. Select INSTR(FIRST_NAME, BINARY'a') from Worker where FIRST_NAME = 'Amitabh';

7. Select distinct length (DEPARTMENT) from Worker;

8. Select REPLACE (FIRST_NAME, 'a','A') from Worker;

9. Select * from Worker order by FIRST_NAME asc;

10. Select from Worker where FIRST_NAME in ('Vipul' , 'Satish');
